<resources>>

    <style name="navyBlue" parent="Theme.AppCompat.NoActionBar">
        <item name="android:colorBackground">@color/background_material_dark_1</item>
        <item name="android:navigationBarColor">@android:color/black</item>
        <item name="android:statusBarColor">@color/blueee</item>
        <item name="android:colorPrimary">@color/primary_material_dark</item>
    </style>

    <style name="navyBlue.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
    </style>

    <style name="BlueTheme" parent="android:Theme.Material.Light" >
        <item name="android:colorPrimary">@color/primary_material_light_1</item>
        <item name="android:colorForeground">@color/foreground_material_light</item>
    </style>


</resources>
